{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azu-eus2-dev-datafactory"
		},
		"blob_storage_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_storage_ls'"
		},
		"ADF_SQL_pl_properties_0_name": {
			"type": "string",
			"defaultValue": "EnumerateContainersList"
		},
		"ADF_SQL_pl_properties_1_name": {
			"type": "string",
			"defaultValue": "ForEachContainer"
		},
		"ADF_SQL_pl_properties_2_name": {
			"type": "string",
			"defaultValue": "Validation_File"
		},
		"ADF_SQL_pl_properties_3_name": {
			"type": "string",
			"defaultValue": "Wait 1 sec"
		},
		"ADF_SQL_pl_properties_parameters_SourceFilePath": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"defaultValue": "/storesales-source"
			}
		},
		"ADF_SQL_pl_properties_parameters_DestinationFilePath": {
			"type": "object",
			"defaultValue": {
				"type": "String",
				"defaultValue": "/storesales-staging"
			}
		},
		"ADF_SQL_pl_properties_parameters_waitTimeInSeconds": {
			"type": "object",
			"defaultValue": {
				"type": "int",
				"defaultValue": 1
			}
		},
		"AzureBlobStorageSinkFileStore_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureBlobStorageSinkFileStore_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().folder"
		},
		"AzureBlobStorageSourceFileStore_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "@dataset().folder"
		},
		"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azu-caes-dev-keyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blob_storage_ls_connectionString": {
						"value": "[parameters('blob_storage_ls_connectionString')]"
					},
					"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorageSinkFileStore_properties_typeProperties_fileName": {
						"value": "[parameters('AzureBlobStorageSinkFileStore_properties_typeProperties_fileName')]"
					},
					"AzureBlobStorageSinkFileStore_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlobStorageSinkFileStore_properties_typeProperties_folderPath')]"
					},
					"AzureBlobStorageSourceFileStore_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlobStorageSourceFileStore_properties_typeProperties_folderPath')]"
					},
					"ADF_SQL_pl_properties_0_name": {
						"value": "[parameters('ADF_SQL_pl_properties_0_name')]"
					},
					"ADF_SQL_pl_properties_1_name": {
						"value": "[parameters('ADF_SQL_pl_properties_1_name')]"
					},
					"ADF_SQL_pl_properties_2_name": {
						"value": "[parameters('ADF_SQL_pl_properties_2_name')]"
					},
					"ADF_SQL_pl_properties_3_name": {
						"value": "[parameters('ADF_SQL_pl_properties_3_name')]"
					},
					"ADF_SQL_pl_properties_parameters_SourceFilePath": {
						"value": "[parameters('ADF_SQL_pl_properties_parameters_SourceFilePath')]"
					},
					"ADF_SQL_pl_properties_parameters_DestinationFilePath": {
						"value": "[parameters('ADF_SQL_pl_properties_parameters_DestinationFilePath')]"
					},
					"ADF_SQL_pl_properties_parameters_waitTimeInSeconds": {
						"value": "[parameters('ADF_SQL_pl_properties_parameters_waitTimeInSeconds')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}